<h1>Listing maps</h1>

<table>
  <tr>
    <th>Uuid</th>
    <th>Data</th>
	<th>Unit Data</th>
    <th>Name</th>
    <th>Players</th>
  </tr>

<% @maps.each do |map| %>
  <tr>
    <td><%=h map.uuid %></td>
    <td><%=h map.data %></td>
	<td><%=h map.unit_data %></td>
    <td><%=h map.name %></td>
    <td><%=h map.players %></td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New map', new_map_path %>